  <%- exposeLocalsToBrowser(); %>
<script src="/js/chat-room.js"></script>
<script src="/js/user-list.js"></script>
<script src="/js/chat-page.js"></script>
<script src="/js/tabs.js"></script>
<script src="https://codepen.io/manikoth/pen/GZKJVJ.js"></script>



    <div class="chat-page">

      <div class="header">
        <h1>Velkommen til Discord Wannabe, <%= username %>!</h1>
        <span class="logout">Klik her for at logge ud</span>
        <br>

          <div class="tab">
          <button class="tablinks" onclick="openCity(event, 'London')">Room 1</button>
          <button class="tablinks" onclick="openCity(event, 'Tokyo')" id="add">Create Room</button>
                  <button id="knap">Knap</button>
        </div>
      </div>


      <div class="main">

        <div class="chat-room-section">
          <div class="chat-transcript-container">
            <div class="chat-transcript"></div>
          </div>
          <div class="chat-controls-container">
            <div class="chat-controls">
              <input class="chat-input" type="text" placeholder="Indtast en besked her..." />
              <button class="send-chat-button">Send besked</button>
            </div>
          </div>
        </div>

        <div class="user-list-section">
          <div class="header">Brugere</div>
          <ul class="user-list">
          </ul>
        </div>

      </div>

    </div>
